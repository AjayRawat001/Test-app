{
    "name": "my-app",
    "version": 2,
    "builds": [{ "src": "/web/next.config.js", "use": "@now/next" }],
    "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=604800" },
      "dest": "/web/static/$1"
    },
   {
      "src": "(.*)",
      "dest": "/web$1",
      "continue": true
   }
   ]
 }